{
    "name": "zf3buch/zf1legacy",
    "description": "ZF1legacy Example Application to migrate to the Zend Framework 3",
    "type": "project",
    "homepage": "https://github.com/zf3buch/zf1legacy",
    "license": "MIT",
    "authors": [
        {
            "name": "Ralf Eggert",
            "email": "ralf@travello.de",
            "homepage": "http://www.travello.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-component-installer": "^1.1.1",
        "zendframework/zend-session": "^2.7",
        "zendframework/zend-expressive": "^1.0",
        "zendframework/zend-expressive-helpers": "^2.0",
        "zendframework/zend-expressive-zendrouter": "^1.2",
        "zendframework/zend-expressive-zendviewrenderer": "^1.1",
        "zendframework/zend-servicemanager": "^3.1",
        "zendframework/zend-config": "^2.6",
        "mtymek/expressive-config-manager": "^0.4.0"
    },
    "require-dev": {
        "filp/whoops": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/",
            "Kitten\\": "module/Kitten/src/"
        }
    },
    "scripts": {
        "serve": "php -S 0.0.0.0:8080 -t public/ public/index.php"
    }
}
